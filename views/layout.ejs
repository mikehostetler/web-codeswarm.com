    <script src="/linker/js/vendor/angular/angular-resource.min.js"></script>
    <script src="/linker/js/vendor/socket.io.js"></script>
    <script src="/linker/js/sails.io.js"></script>
    <script src="/linker/js/strider.js"></script>
    <script src="/linker/js/app.js"></script>
    <script src="/linker/js/controllers/dashboard.js"></script>
    <script src="/linker/js/controllers/error.js"></script>
    <script src="/linker/js/controllers/login.js"></script>
    <!--SCRIPTS END-->
  </body>
</html>
